{
  "name": "express-crud-todo",
  "version": "1.0.0",
  "description": "a todo api",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "jest --forceExit --coverage --testPathIgnorePatterns=performance",
    "test:perf": "jest --forceExit tests/performance.test.js"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 70,
        "functions": 70,
        "branches": 70,
        "statements": 70
      }
    },
    "collectCoverageFrom": [
      "app.js",
      "routes/**/*.js",
      "database/**/*.js"
    ]
  },
  "dependencies": {
    "dotenv": "^17.3.1",
    "express": "^4.17.1",
    "lodash": "4.17.23",
    "moment": "2.29.4",
    "sql.js": "^1.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-dist": "^5.31.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "autocannon": "^8.0.0",
    "eslint": "^10.0.2",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "supertest": "^7.2.2"
  },
  "overrides": {
    "glob": "^11.0.0",
    "minimatch": "^10.2.1",
    "test-exclude": "^8.0.0"
  }
}
